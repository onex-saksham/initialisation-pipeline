{
    "deploy": {
        "zookeeper": "__zk__",
        "kafka": "__kf__",
        "nifi": "__nf__",
        "nifi_registry": "__nr__",
        "doris_fe": "__fe__",
        "doris_be": "__be__",
        "connect_fe_be": "__co__",
        "node_exporter": "__nx__",
        "kafka_exporter": "__kx__",
        "prometheus": "__pm__",
        "grafana": "__gf__",
        "health_reports": "__hr__",
        "recon": "__rc__",
        "jobs": "__jb__",
        "api": "__ap__",
        "nginx": "__ng__",
        "schema_registry":"__sr__"

    },
    "releases": {
        "new_version": "__new_version__",
        "current_version": "__current_version__",
        "tenant_id": [
            "__tenant_id__"
        ]
    },
    "base_user": "jenkins",
    "user": "__ODP_user__",
    "deployment_type": "__deployment_type__",
    "deployment_path": "__deployment_path__",
    "ssh_port": "__ssh_port__",
    "deploy_whatsapp": "__wa__",
    "deploy_sms": "__sms__",
    "deploy_rcs": "__rcs__",
    "zookeeper": {
        "node_ip": [
            "__zookeeper_ip__"
        ],
        "deployment_path": "",
        "properties": {
            "storage": "/data/zookeeper-storage"
        },
        "ports": {
            "client": 2181,
            "peer_to_peer": 2888,
            "leader_election": 3888
        },
        "java_version": "17",
        "python_version": "3.12.0"
    },
    "kraft_controller": {
        "deployment_path": "",
        "properties": {
            "storage": "/data/kraft-controller-storage",
            "num_partitions": {
                "single": 3,
                "multi": 7
            },
            "replication_factor": {
                "single": 1,
                "multi": 3
            },
            "log_retention_hours": 72
        },
        "ports": {
            "listener": 9093
        }
    },
    "kafka": {
        "broker_ip": [
            "__kafka_ip__"
        ],
        "deployment_path": "",
        "properties": {
            "storage": "/data/kafka-storage",
            "num_partitions": {
                "single": 3,
                "multi": 7
            },
            "replication_factor": {
                "single": 1,
                "multi": 3
            },
            "log_retention_hours": 72
        },
        "ports": {
            "jmx": 9091,
            "listener": 9092
        },
        "java_version": "17",
        "python_version": "3.12.0"
    },
    "schema_registry": {
        "node_ip": [
            "__schema_registry_ip__"
        ],
        "deployment_path": "",
        "ports": {
            "listener": 8081
        }
    },
    "nifi": {
        "node_ip": [
            "__nifi_ip__"
        ],
        "deployment_path": "",
        "properties": {
            "zk_root_node": "/nifi",
            "min_heap_memory": 4,
            "max_heap_memory": 8
        },
        "ports": {
            "node_load_balance": 6342,
            "http": 8080,
            "remote_input_socket": 10443,
            "registry": 18080,
            "node_protocol": 11443
        },
        "java_version": "21",
        "python_version": "3.12.0"
    },
    "doris_fe": {
        "node_ip": [
            "__doris_fe_ip__"
        ],
        "deployment_path": "",
        "properties": {
            "log": "${DORIS_HOME}/log"
        },
        "ports": {
            "http": 8030,
            "edit_log": 9010,
            "rpc": 9020,
            "query": 9030
        },
        "java_version": "17",
        "python_version": "3.12.0"
    },
    "doris_be": {
        "node_ip": [
            "__doris_be_ip__"
        ],
        "deployment_path": "",
        "properties": {
            "log": "${DORIS_HOME}/log",
            "storage": "/data/doris-be-storage"
        },
        "ports": {
            "webserver": 8040,
            "brpc": 8060,
            "heartbeat_service": 9050,
            "be": 9060
        },
        "java_version": "17",
        "python_version": "3.12.0"
    },
    "node_exporter": {
        "deployment_path": "",
        "for": [
            "zookeeper",
            "kafka",
            "nifi",
            "doris_fe",
            "doris_be",
            "monitoring",
            "api"
        ],
        "ports": {
            "listener": 9100
        }
    },
    "kafka_exporter": {
        "deployment_path": "",
        "ports": {
            "listener": 9308
        }
    },
    "monitoring": {
        "node_ip": "__monitoring_ip__",
        "deployment_path": "",
        "properties": {
            "storage": "/data/grafana-storage"
        },
        "ports": {
            "prometheus": 9090,
            "grafana": 3000
        },
        "java_version": "17",
        "python_version": "3.12.0",
        "smtp": {
            "from_address": "__sender_email__",
            "password": "__sender_password__"
        }
    },
    "api": {
        "node_ip": [
            "__api_ip__"
        ],
        "deployment_path": "",
        "ports": {
            "sms": 12000,
            "warcs": 12001
        },
        "deployment_version": "__deployment_version__",
        "remote_jobs": "__remote_jobs__",
        "use_nginx": "__use_nginx__",
        "erlang_registry_ip": "__erlang_registry_ip__",
        "java_version": "17",
        "python_version": "3.12.0"
    },
    
    "backend_job": {
        "node_ip": ["__backend_job_ip__"],
        "ID_DESC_URL": "__ID_DESC_URL__",
        "ZIP_HOST_IP":"__ZIP_HOST_IP__",
        "ZIP_USERNAME":"__ZIP_USERNAME__",
        "ZIP_SSH":"__ZIP_SSH__",
        "IS_SCP_REQUIRED":"__IS_SCP_REQUIRED__",
        "BACKEND_JOB_USER": "__backend_JOB_USER__",
        "deployment_path": "",
        "java_version": "17",
        "python_version": "3.12.0"
    },
    "nginx": {
        "node_ip": "__nginx_ip__",
        "deployment_path": "",
        "services": [
            "doris_fe",
            "api_sms",
            "api_warcs",
            "schema_registry"
        ],
        "ports": {
            "doris_fe": 1080,
            "api_sms": 1081,
            "api_warcs": 1082,
            "schema_registry": 1083
    }
    },
    "health_report": {
        "retry_count": 3,
        "retry_interval": 300
    }

}